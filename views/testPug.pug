extends layout
block content 
	.row
		form( action="#") 
			.col.s12.m5
				.row 
					.file-field.input-field
						.btn
							span File For Pattern
							input#inputPattern( type="file")
						.file-path-wrapper
							input(
								class="file-path validate"
								type="text"
								)

		form( action="#") 
			.col.s12.m5
				.row 
					.file-field.input-field
						.btn
							span File For form
							input#inputForm( type="file")
						.file-path-wrapper
							input(
								class="file-path validate"
								type="text"
								)
	table
		tr
			td
				svg(id="shape")
			td
				table
					tr
						a(id="download", class="waves-effect waves-light btn", href="#") Telecharger
	table
		tr
			td
				p espacement en X
				form(action="#")
					p(class="range-field")
						input(type="range", id="espacementX", min="0", max="100")
			td
				p espacement en Y
				form(action="#")
					p(class="range-field")
						input(type="range", id="espacementY", min="0", max="100")
		tr
			td
				p nombre de répétition en X
				form(action="#")
					p(class="range-field")
						input(type="range", id="numberX", min="0", max="50")
			td
				p nombre de répétition en Y
				form(action="#")
					p(class="range-field")
						input(type="range", id="numberY", min="0", max="50")



